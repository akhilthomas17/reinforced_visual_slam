<launch>
    
    <node name="deepTAM_tracker" pkg="reinforced_visual_slam" type="deepTAM_tracker.py" respawn="true" output="screen"/>

    <node name="lsd_node_reinforced_dataset" pkg="lsd_slam_core" type="deepTAM_dataset_slam" respawn="true" output="screen">

        <param name="hz" value="0" />

        <param name="calib" value="/misc/lmbraid19/thomasa/rosbuild_ws/packages/lsd_slam/lsd_slam_core/calib/OpenCV_example_calib.cfg"/>
        <param name="files" value="/misc/lmbraid17/zhouh/data/rgbd_tum/rgbd_dataset_freiburg1_teddy/rgb.txt"/>
        <param name="doSLAM" value="false"/>
        <param name="/Debug/plotStereoImages" value="true"/>
        <param name="/Debug/printPropagationStatistics" value="true"/>
        <param name="/Debug/printFillHolesStatistics" value="true"/>
        <param name="/Debug/printObserveStatistics" value="true"/>
        <param name="/Debug/printObservePurgeStatistics" value="true"/>
        <param name="/Debug/printRegularizeStatistics" value="true"/>
        <param name="/Debug/printLineStereoStatistics" value="true"/>
        <param name="/Debug/printLineStereoFails" value="true"/>


        <remap from="/lsd_slam/debug" to="/lsd_slam_reinforced/debug"/>
        <remap from="/lsd_slam/graph" to="/lsd_slam_reinforced/graph"/>
        <remap from="/lsd_slam/keyframes" to="/lsd_slam_reinforced/keyframes"/>
        <remap from="/lsd_slam/liveframes" to="/lsd_slam_reinforced/liveframes"/>

    </node>

    <node name="lsd_node_reinforced_viewer" pkg="lsd_slam_viewer" type="viewer" respawn="true" output="screen">
        <remap from="/lsd_slam/debug" to="/lsd_slam_reinforced/debug"/>
        <remap from="/lsd_slam/graph" to="/lsd_slam_reinforced/graph"/>
        <remap from="/lsd_slam/keyframes" to="/lsd_slam_reinforced/keyframes"/>
        <remap from="/lsd_slam/liveframes" to="/lsd_slam_reinforced/liveframes"/>
    </node>



